<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><%@taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%><jsp:include page="header.jsp"></jsp:include>				<!-- BEGIN PAGE CONTENT-->				<div class="row-fluid">					<div class="span12 blog-page">						<div class="row-fluid">							<div class="span9 article-block">								<h3>${log.title }</h3>								<div class="blog-tag-data">									<div class="row-fluid">										<div class="span6">											<ul class="unstyled inline blog-tags">												<li>													<i class="icon-tags"></i> 													<c:forTokens									items="${log.keywords}" delims="," var="tag">									<a href="${rurl}post/tag/${tag}" rel="tag">${tag}</a></c:forTokens>												</li>											</ul>										</div>										<div class="span6 blog-tag-data-inner">											<ul class="unstyled inline">											<li><i class="icon-calendar"></i> <a href="#">${log.releaseTime.year+1900}年${log.releaseTime.month+1}月${log.releaseTime.date}日</a></li>											<li><i class="icon-comments"></i> <a href="${rurl}post/${log.alias}#comments">${log.commentSize} Comments</a></li>											</ul>										</div>									</div>								</div>								<!--end news-tag-data-->								<div>										<P>${log.content}</P>								</div>								<hr>							<!-- Duoshuo Comment BEGIN -->										<div class="ds-thread" data-thread-key="${log.logId}"											data-title="${log.tilte }"											data-url="${rurl}post/${log.alias}"></div>										<script type="text/javascript">										var duoshuoQuery = {short_name:"xchun"};											(function() {												var ds = document.createElement('script');												ds.type = 'text/javascript';ds.async = true;												ds.src = 'http://static.duoshuo.com/embed.js';												ds.charset = 'UTF-8';												(document.getElementsByTagName('head')[0] 												|| document.getElementsByTagName('body')[0]).appendChild(ds);											})();											</script>								<!-- Duoshuo Comment END -->							</div>							<!--end span9-->							<div class="span3 blog-sidebar">								 <jsp:include page="plugs.jsp"></jsp:include>							</div>							<!--end span3-->						</div>					</div>				</div> <jsp:include page="footer.jsp"></jsp:include>